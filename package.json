{
  "name": "zz-hooks",
  "version": "1.0.0",
  "private": true,
  "description": "吱吱 React 业务 Hooks",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "prepare": "husky install",
    "init": "pnpm install && pnpm run build",
    "start": "pnpm run dev",
    "clean-dist": "rimraf 'packages/hooks/{lib,es,node_modules,dist}'",
    "clean": "pnpm run clean-dist && rimraf node_modules",
    "build": "pnpm -r --filter=./packages/* run build",
    "doc:dev": "export NODE_OPTIONS=--openssl-legacy-provider && dumi dev",
    "doc:build": "export NODE_OPTIONS=--openssl-legacy-provider && dumi build",
    "doc:deploy": "sh deploy.sh",
    "test": "jest",
    "coveralls": "jest --coverage --coverageReporters=text-lcov | coveralls",
    "pub": "pnpm run build && pnpm -r --filter=./packages/* publish",
    "pub:beta": "pnpm run build && pnpm -r --filter=./packages/* publish --tag beta",
    "zz-fe-lint-scan": "zz-fe-lint scan",
    "zz-fe-lint-fix": "zz-fe-lint fix"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "zz-fe-lint": "^0.0.2",
    "@ant-design/icons": "^5.0.1",
    "@babel/cli": "^7.10.1",
    "@babel/core": "^7.10.2",
    "@babel/plugin-transform-runtime": "^7.19.6",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.0.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@types/node": "20.10.6",
    "@types/jest": "^29.4.0",
    "@types/mockjs": "^1.0.7",
    "@types/react-router": "^5.1.19",
    "@umijs/fabric": "^2.1.0",
    "antd": "^5.2.1",
    "babel-plugin-import": "^1.12.0",
    "coveralls": "^3.1.1",
    "cross-env": "^7.0.3",
    "del": "^5.1.0",
    "dumi": "^1.1.48",
    "eslint": "^7.2.0",
    "eslint-plugin-react-hooks": "^4.0.8",
    "fast-glob": "^3.2.11",
    "fs-extra": "^10.0.1",
    "gray-matter": "^4.0.3",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0",
    "gulp-typescript": "^6.0.0-alpha.1",
    "husky": "^8.0.0",
    "jest": "^29.4.1",
    "jest-environment-jsdom": "^29.4.1",
    "jest-localstorage-mock": "^2.4.18",
    "mockjs": "^1.1.0",
    "prettier": "^2.0.5",
    "pretty-quick": "^3.1.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-drag-listview": "^0.1.6",
    "react-router": "^6.4.2",
    "react-shadow": "^19.0.3",
    "rimraf": "^3.0.2",
    "surge": "^0.21.3",
    "tslib": "^2.4.1",
    "ts-jest": "^29.0.5",
    "typescript": "^5.1.3",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.10",
    "webpack-merge": "^4.2.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "zz-fe-lint commit-file-scan",
      "commit-msg": "zz-fe-lint commit-msg-scan"
    }
  },
  "directories": {
    "doc": "docs"
  },
  "dependencies": {
    "eslint-ast-utils": "^1.1.0",
    "eslint-config-egg": "^10.0.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-config-zz": "^1.0.0",
    "eslint-formatter-pretty": "^4.1.0",
    "eslint-import-resolver-node": "^0.3.9",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-module-utils": "^2.8.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-eggache": "^1.0.0",
    "eslint-plugin-es": "^3.0.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^24.7.0",
    "eslint-plugin-jsdoc": "^37.9.7",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-unicorn": "^20.1.0",
    "eslint-plugin-vue": "^7.20.0",
    "eslint-rule-composer": "^0.3.0",
    "eslint-rule-docs": "^1.1.235",
    "eslint-scope": "^5.1.1",
    "eslint-template-visitor": "^2.3.2",
    "eslint-utils": "^2.1.0",
    "eslint-visitor-keys": "^2.1.0",
    "prettier-linter-helpers": "^1.0.0",
    "prettier-plugin-packagejson": "^2.3.0",
    "prettier-plugin-two-style-order": "^1.0.1",
    "stylelint-config-prettier": "^8.0.2",
    "vue-eslint-parser": "^7.11.0"
  }
}
